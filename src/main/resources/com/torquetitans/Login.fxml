<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<VBox fx:controller="GarageApp.LoginController" spacing="10" alignment="CENTER" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label text="USERS" />
      <TextField fx:id="usernameField" />
      <Label text="PASSWORD:" />
      <PasswordField fx:id="passwordField" />
      <Button fx:id="loginButton" text="LOGIN" />
      <Button fx:id="registerButton" text="SIGN UP" />
   </children>
</VBox>